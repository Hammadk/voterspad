<h1> <%= @election.name %> </h1>
<h3> <%= @election.year %> </h3>

<table
  data-toggle="table"
  data-search="true"
  data-show-columns="true"
  data-mobile-responsive="true"
>
    <thead>
        <tr>
            <th>Questions</th>
            <th>Candidate 1</th>
            <th>Candidate 2</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>Question 1 Text</td>
            <td>Candidate 1 answer to question 1</td>
            <td>Candidate 2 answer to question 1</td>
        </tr>
        <tr>
            <td>Question 2 Text</td>
            <td>Candidate 1 answer to question 2</td>
            <td>Candidate 2 answer to question 2</td>
        </tr>
    </tbody>
</table>
